!!!
%html
	%head
		%title= title
		= render 'layouts/stylesheets'
		= javascript_include_tag :defaults
		= javascript_include_tag "jquery.hotkeys", "jquery.watermark", 'jquery-ui-1.8.16.custom.min'
		= csrf_meta_tag
	%body
		.container
			= render 'layouts/header'
			%section.round
				= yield       
		= render 'layouts/footer'
		= debug(params) if Rails.env.development?